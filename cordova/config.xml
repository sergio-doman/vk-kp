<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sergiydomanitskiy.vk-kp" version="1.0.0">
  <name>Вечірній Кам'янець</name>
  <description>
    Неофіційна мобільна версія Кам'янець-Подільської онлайн газети
  </description>
  <content src="package.json" />
  <preference name="Fullscreen" value="false" />
  <platform name="android">
    <preference name="android-minSdkVersion" value="16" />
    <icon src="res/android/ldpi.png" density="ldpi" />
    <icon src="res/android/mdpi.png" density="mdpi" />
    <icon src="res/android/hdpi.png" density="hdpi" />
    <icon src="res/android/xhdpi.png" density="xhdpi" />
    <icon src="res/android/xxhdpi.png" density="xxhdpi" />
    <icon src="res/android/xxxhdpi.png" density="xxxhdpi" />
  </platform>
  <plugin name="cordova-plugin-dialogs" version="1.2.1" />
  <plugin name="cordova-plugin-x-toast" version="2.5.2" />
  <plugin name="cordova-plugin-actionsheet" version="2.3.1" />
  <plugin name="cordova-plugin-x-socialsharing" version="5.1.3" />
  <plugin name="com.cmackay.plugins.googleanalytics" version="0.1.22" />
</widget>
